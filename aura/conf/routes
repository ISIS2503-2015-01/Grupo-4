# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Paciente

DELETE    /paciente/:id       controllers.PacienteController.eliminarPaciente(id:Long)
POST     /paciente                         controllers.PacienteController.crearPaciente()
GET     /paciente/all                   controllers.PacienteController.getAll()
GET     /pacienteId/:id                     controllers.PacienteController.buscarUnoId(id:Long)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
